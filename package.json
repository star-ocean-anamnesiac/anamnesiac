{
  "name": "anamnesiac",
  "description": "A compendium of information for Star Ocean: Anamnesis",
  "version": "1.0.0",
  "author": "Kyle Kemp <kyle@seiyria.com>",
  "homepage": "https://anamnesiac.seiyria.com",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --port 6789 --host 0.0.0.0",
    "build": "npm run consolidate && npm run prepare:prodimages && ng build --prod && npm run prepare:api",
    "lint": "ng lint",
    "validate": "cross-env TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ava-ts src/test/**/*.test.ts",
    "test": "npm run validate && npm run lint && npm run build && npm run analysis:dump",
    "publish": "node scripts/sync",
    "consolidate": "npm run prepare:json && npm run prepare:pack:appicons && npm run prepare:pack:itemicons && npm run prepare:pack:charicons && npm run prepare:pack:skillicons && npm run prepare:pack:rushicons && npm run prepare:pack:bossicons && npm run prepare:create:cards && npm run prepare:pngcrush",
    "prepare:json": "node scripts/consolidate-data",
    "prepare:pack:appicons": "node scripts/pack-appicons",
    "prepare:pack:bossicons": "node scripts/pack-bossicons",
    "prepare:pack:itemicons": "node scripts/pack-itemicons",
    "prepare:pack:charicons": "node scripts/pack-charicons",
    "prepare:pack:skillicons": "node scripts/pack-skillicons",
    "prepare:pack:rushicons": "node scripts/pack-rushicons",
    "prepare:create:cards": "node scripts/card/compile && node scripts/card/pup",
    "prepare:pngcrush": "node scripts/pngcrush",
    "prepare:cleanup": "node scripts/build-cleanup",
    "prepare:prodimages": "node scripts/prodimages",
    "prepare:api": "node scripts/build-api",
    "analysis:dump": "node scripts/analysis/chars && node scripts/analysis/items"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/pwa": "^0.11.0",
    "@angular/router": "~7.0.0",
    "@angular/service-worker": "~7.0.0",
    "@ionic-native/core": "5.0.0-beta.21",
    "@ionic-native/splash-screen": "5.0.0-beta.21",
    "@ionic-native/status-bar": "5.0.0-beta.21",
    "@ionic/angular": "^4.0.0-beta.16",
    "core-js": "^2.5.4",
    "imagemin-webp": "^5.0.0",
    "lazysizes": "^4.1.6",
    "lodash": "^4.17.11",
    "markdown": "^0.5.0",
    "ngx-filter-pipe": "^2.1.2",
    "ngx-webstorage": "^3.0.0-beta.14",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.10.0",
    "@angular-devkit/build-angular": "~14.2.9",
    "@angular-devkit/core": "~7.0.0",
    "@angular-devkit/schematics": "~7.0.0",
    "@angular/cli": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@ionic/angular-toolkit": "~1.2.0",
    "@types/node": "~10.12.0",
    "ava": "^0.25.0",
    "ava-ts": "^0.25.2",
    "base64-img": "^1.0.4",
    "bluebird": "^3.5.3",
    "codelyzer": "~4.5.0",
    "cross-env": "^5.2.0",
    "google-spreadsheet": "^2.0.7",
    "image-size": "^0.7.1",
    "imagemin": "^6.1.0",
    "imagemin-pngquant": "^7.0.0",
    "js-yaml": "^3.12.1",
    "puppeteer": "^1.13.0",
    "rimraf": "^2.6.3",
    "spritesheet-js": "^1.2.6",
    "ts-node": "^7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
